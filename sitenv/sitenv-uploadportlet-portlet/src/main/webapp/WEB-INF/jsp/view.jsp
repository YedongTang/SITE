orm>      		<script type="text/javascript">		var url = '${urlAction}';	</script>      	      	      	      	<div id="ValidationResult" style="display:none">      		<div id="tabs">			  <ul>			    <li><a href="#tabs-1">Validation Result</a></li>			    <li><a href="#tabs-2">Original CCDA</a></li>			    <li><a href="#tabs-3">Smart CCDA Result</a></li>			  </ul>			  <div id="tabs-1">			    <h2>Content heading 1</h2>			  </div>			  <div id="tabs-2">			    <h2>Place holder</h2>			    <p>Under construction.</p>			  </div>			  <div id="tabs-3">			  </div>			</div>      	</div>      	      	<div id="reportSaveAsQuestion" style="display:none; cursor: default">      		<form id="downloadtest" action="<%= downloadCCDAAction %>" method="POST">      		<textarea id="reportContent" name="reportContent" style="display:none;">      		</textarea>			<br>	    	<fieldset>				<legend><b>Save your report as:</b></legend>					<div>					<div class="radio">					<input type="radio" id="pdf_report_type" name="report_type" checked value="PDF" style="border:3px solid green">					</div>					<label for="pdf_report_type">PDF</label>				</div>								<div>					<div class="radio">					<input type="radio" id="html_report_type" name="report_type" value="HTML" disabled>					</div>					<label for="html_report_type">HTML</label>				</div>								<div>					<div class="radio">					<input type="radio" id="txt_report_type" name="report_type" value="TXT" disabled>					</div>					<label for="txt_report_type">TXT</label>				</div>				</fieldset>	        <button id="yes">OK</button>	        <button id="no">Cancel</button>	        </form>       	</div>      	      	      <portlet:renderURL var="viewCaseURL">          <portlet:param name="jspPage" value="/view2.jsp" />      </portlet:renderURL>            